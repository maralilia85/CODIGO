<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:p="http://primefaces.org/ui">

         <h:form id="formDonacion">
        <p:fieldset legend="Realizar Ingreso por Referencia">
            <p:fieldset legend="Cabecera Ingreso">
                <p:selectOneRadio value="#" columns="1">
                <f:selectItem itemLabel="Donación" itemValue="D" />
                    </p:selectOneRadio>
                    <p:panelGrid columns="4" layout="grid" style="border: none; background: none">
                        
                         
                        <p:inputText placeholder="#{etiquetas['noOfi']}  " 
                                 autocomplete="off" 
                                 onblur="value = value.toUpperCase();" 
                                 required="true">
                    </p:inputText>
                         
                
                      
                         <p:calendar  placeholder="#{etiquetas['fecha']}" 
                                required="true"  size="12"
                                pattern="dd/MM/yyyy" showOn="button" navigator="true" locale="es">
                    </p:calendar>
                        
                         
                          <p:selectOneMenu  value="#" 
                                     required="true">
                        <f:selectItem itemLabel="Tipo" itemValue="" noSelectionOption="true"/>
                        <f:selectItem  itemLabel="Insumo Médico" itemValue="S" />  

                    </p:selectOneMenu> 
                
                    </p:panelGrid>
     
            </p:fieldset>
                    
<p:commandButton value="Detalle" icon="#{botones['boton.iconnuevo']}"
                                     action="#"
                                     update="pResultado"
                                     title="#{botones['title.nuevo']}">
                    </p:commandButton>
               
                <p:panel id="pResultado" style="border: hidden">
                <p:dataTable id="dtDetalleFactura" value="#" var="art"
                             paginatorPosition="top"
                             reflow="true"
                             paginatorTemplate="{PreviousPageLink} {PageLinks} {NextPageLink}"
                             paginator="true" rows="10" rowIndexVar="index" resizableColumns="true" 
                             editable="true" editMode="cell" emptyMessage="No se encontraron resultados">

                    <p:ajax event="cellEdit" update="dtDetalleFactura"/>


                   
                   

                    <p:column headerText="#{etiquetas['codigo']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <p:cellEditor>

                            <f:facet name="output">
                                <h:outputText value=""/>
                            </f:facet>
                            <f:facet name="input">
                                <p:inputText  onblur="value = value.toUpperCase();" 
                                              placeholder="Generico" 
                                              value="#"
                                              autocomplete="off" required="true">
                                </p:inputText>
                                
                            </f:facet>
                        </p:cellEditor>
                    </p:column>
                    <p:column headerText="#{etiquetas['descripcion']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <h:outputText value=""/>
                  
                    </p:column>
                    <p:column headerText="#{etiquetas['referencia']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                  <h:outputText value=""/>
                    </p:column>
                    <p:column headerText="#{etiquetas['saldo']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                     <h:outputText value=""/>    
                      
                    </p:column>
                      <p:column headerText="#{etiquetas['Iva']}" filterBy="#"
                              filterMatchMode="contains" class="Wid5" >
                  <h:outputText value="0"/>
                    </p:column>
                    <p:column headerText="#{etiquetas['porDesc']}" filterBy="#"
                              filterMatchMode="contains" class="Wid5" >
                  <h:outputText value="0.00"/>
                    </p:column>
                    <p:column headerText="#{etiquetas['cantIngreso']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <p:cellEditor>

                            <f:facet name="output">
                                <h:outputText value=""/>
                            </f:facet>
                            <f:facet name="input">
                                <p:inputText  onblur="value = value.toUpperCase();" 
                                              placeholder="#{etiquetas['cantIngreso']}" 
                                              value="#"
                                              autocomplete="off" required="true">
                                </p:inputText>
                                
                            </f:facet>
                        </p:cellEditor>
                      
                    </p:column>
                    <p:column headerText="#{etiquetas['total']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                  <h:outputText value="0.00"/>
                    </p:column>
                    <p:column headerText="#{etiquetas['lote']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <p:cellEditor>

                            <f:facet name="output">
                                <h:outputText value=""/>
                            </f:facet>
                            <f:facet name="input">
                                <p:inputText  onblur="value = value.toUpperCase();" 
                                              placeholder="#{etiquetas['lote']}" 
                                              value="#"
                                              autocomplete="off" required="true">
                                </p:inputText>
                                
                            </f:facet>
                        </p:cellEditor>
                      
                    </p:column>
                    <p:column headerText="#{etiquetas['regSani']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <p:cellEditor>

                            <f:facet name="output">
                                <h:outputText value=""/>
                            </f:facet>
                            <f:facet name="input">
                                <p:inputText  onblur="value = value.toUpperCase();" 
                                              placeholder="#{etiquetas['lote']}" 
                                              value="#"
                                              autocomplete="off" required="true">
                                </p:inputText>
                                
                            </f:facet>
                        </p:cellEditor>
                      
                    </p:column>
                    <p:column headerText="#{etiquetas['fechaElab']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <p:cellEditor>

                            <f:facet name="output">
                                <h:outputText value=""/>
                            </f:facet>
                            <f:facet name="input">
                                <p:calendar  placeholder="dd/MM/yyyy" 
                                required="true"  size="12"
                                pattern="dd/MM/yyyy" showOn="button" navigator="true" locale="es">
                    </p:calendar>
                                
                            </f:facet>
                        </p:cellEditor>
                      
                    </p:column>
                    <p:column headerText="#{etiquetas['fechaCaduc']}" filterBy="#"
                              filterMatchMode="contains" class="Wid10" >
                        <p:cellEditor>

                            <f:facet name="output">
                                <h:outputText value=""/>
                            </f:facet>
                            <f:facet name="input">
                                <p:calendar  placeholder="dd/MM/yyyy" 
                                required="true"  size="12"
                                pattern="dd/MM/yyyy" showOn="button" navigator="true" locale="es">
                    </p:calendar>
                                
                            </f:facet>
                        </p:cellEditor>
                      
                    </p:column>
                     <p:column headerText="#{etiquetas['opciones']}" class="Wid5 TexAlCenter">
                         <p:commandLink id="dynaButton" class="icon-compose-1"
                                        title="#{botones['title.seleccion']}">
                             <f:setPropertyActionListener value="#{art}" target="#" />
                        </p:commandLink>
                         <p:menu overlay="true" trigger="dynaButton" my="left top" at="left bottom">
                            <p:submenu label="Mantenimiento">
                                
                                <p:menuitem value="#{botones['inactivar']}"  icon="#{botones['boton.iconinactiva']}" action="#"
                                            title="#{botones['title.inactiva']}" />
                                <p:menuitem value="Eliminar"  icon="#{botones['boton.iconeliminar']}" action="#"
                                            title="#{botones['title.eliminar']}" />
                            </p:submenu>
                           
                           

                        </p:menu>
                       

                    </p:column>
                   
                    
                    
                     

                </p:dataTable>                     
            </p:panel>
      
           
         
            
        </p:fieldset>
        
    </h:form>
        
 
        
       
  

</ui:composition>